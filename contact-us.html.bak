                    });
                    $('.footer-chev').removeClass('rotate');
                    chevron.addClass("rotate");
                } else {
                    content.css({
                        height: 0,
                        margin: 0 // Remove margin
                    });
                    chevron.removeClass("rotate");
                }
            });
        });


        const div1 = document.querySelector('.header');
        const div2 = document.querySelector('.header-top-container');
        const div3 = document.querySelector('.header-cta-bar');

        div1.addEventListener('mouseout', () => {
            div2.style.height = '0';
        });

        div3.addEventListener('mouseout', () => {
            div2.style.height = '0';
        });

        div1.addEventListener('mouseover', () => {
            div2.style.height = '35px'; // Reset the height when the mouse is back on div1
        });

        div3.addEventListener('mouseover', () => {
            div2.style.height = '35px'; // Reset the height when the mouse is back on div1
        });
    </script>
</body>

</html>